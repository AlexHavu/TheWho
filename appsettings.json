{
  "Serilog": {
    "Using": [
      "TipaltiCoreLogger",
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
      //"Serilog.Filters.Expressions"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Error",
        "System": "Error",
        "HealthChecks": "Error",
        "Quartz": "Information",
        "IdentityServer4": "Warning"
      }
    },
    "Enrich": [
      "WithEnvironmentUserName",
      "FromLogContext",
      "WithMachineName",
      "WithThreadId",
      {
        "Name": "RenameProperties",
        "Args": {
          "renameProperties": {
            "MachineName": "host"
          }
        }
      }
    ]
  },
  "Quartz": {
    "IsEnabled": true,
    "DataSourceConnectionStringName": "TipaltiConnectionString"
  },
  "AllowedHosts": "*",
  "Telemetry": {
    "IsEnabled": true,
    "ServiceName": "Tipalti.TheWho"
  }
}
